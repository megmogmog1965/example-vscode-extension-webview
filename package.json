{
  "name": "example-vscode-extension-webview",
  "displayName": "example-vscode-extension-webview",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.93.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "webview/context": [
        {
          "command": "example-vscode-extension-webview.helloWorld",
          "when": "webviewId == 'example-vscode-extension-webview'"
        }
      ]
    },
    "commands": [
      {
        "command": "example-vscode-extension-webview.helloWorld",
        "title": "Hello World",
        "category": "example-vscode-extension-webview"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}
